{
  "compilerOptions": {
    "target": "es2016",
    "lib": [
      "es5",
      "dom",
      "es2015.promise"
    ],
    "module": "commonjs",
    "baseUrl": "./",
    "types": [
      "cypress",
      "node"
    ],
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "**/*.ts"
  ]
}
